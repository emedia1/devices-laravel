{
    "name": "emedia/devices-laravel",
    "description": "Package for device authentication",
    "type": "project",
    "repositories": [
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/quickdata-laravel.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/laravel-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/php-helpers.git"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.7|^6.0",
        "illuminate/database": "^5.7|^6.0",
        "emedia/quick-data-laravel": "~2.0|~3.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Chamal",
            "email": "chamalw.emedia@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "EMedia\\Devices\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EMedia\\Devices\\DeviceAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
